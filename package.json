    "load-grunt-tasks": "^0.6.0",
    "lodash": "~2.4.1",
    "http-server": "^0.6.1",
    "phantomjs": "^1.9.7-8",
    "time-grunt": "^0.4.0",
    "@itunes/rio-grunt-plugin": "^0.3.29",
    "grunt-text-replace": "^0.3.12",
    "@itunes/grunt-contrib-cssmin": "~0.10.0",
    "@itunes/grunt-usemin": "~2.4.1",
    "grunt": "~0.4.5"
